{
    "rules": {
        "no-unused-vars": "off",
        "@typescript-eslint/no-unused-vars": "error",
        "no-useless-constructor": "off",
        "@typescript-eslint/no-useless-constructor": "error",
        "semi": [
            "error",
            "always"
        ],
        "space-before-function-paren": [
            "error",
            "never"
        ]
    },
    "plugins": [
        "@typescript-eslint/eslint-plugin"
    ],
    "parser": "@typescript-eslint/parser"
}
